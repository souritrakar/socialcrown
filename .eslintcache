        <br/>\n            <TextField\n              style={{ width: \"50%\" }}\n              \n              label=\"Name\"\n              name=\"name\"\n              onChange={(e) => this.setState({ username: e.target.value })}\n              required\n            />\n            <br />\n\n            <h2>Email</h2>\n            <br/>\n            <TextField\n              style={{ width: \"50%\" }}\n              type=\"email\"\n              label=\"Email\"\n              name=\"email\"\n              required\n              onChange={(e) => this.setState({ email: e.target.value })}\n            />\n            <br />\n            <h2>Password</h2>\n            <br/>\n            <TextField\n              style={{ width: \"50%\" }}\n              type=\"password\"\n              label=\"Password\"\n              name=\"password\"\n              required\n              onChange={(e) => this.setState({ password: e.target.value })}\n            />\n         \n    \n            <br />\n           \n\n            <div\n              className=\"button_cont\"\n              align=\"center\"\n              style={{ marginTop: 30 }}\n              onClick={()=>{this.signUp(this.state.email,this.state.password)}}\n            >\n              <a\n                className=\"example_e\"\n                \n                target=\"_blank\"\n                rel=\"nofollow noopener\"\n              >\n                <strong style={{fontFamily:\"Raleway, sans-serif\"}}>SIGN UP</strong>\n              </a>\n            </div>\n           \n           \n            <div style={{display:\"flex\",flexDirection:\"row\",marginRight:\"9%\"}} className=\"googlesignup\">\n              <FcGoogle size={50} style={{marginRight:\"5%\",marginTop:\"20%\"}}/> \n              \n              <div\n              style={{marginBottom:\"5%\"}}\n              className=\"button_cont\"\n              align=\"center\"\n              style={{ marginTop: 30,color:\"red\" }}\n              onClick